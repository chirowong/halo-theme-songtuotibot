<#include "module/macro_apple.ftl">
<@head customcss="palm" currentmenu="企业产品"/>
    <!-- 主体内容栏-->
    <div class="content">
        <!--导航图-->
        <div class="section palm-nav" >
            <div class="palm-nav-text">
                <h1>高精度表面3D触觉数字化</h1>
                <h3>
                    Palm Mobile系列
                </h3>
            </div>
        </div>
        <div class="section palm-feature">
                <div class="palm-feature-row">
                    <div class="palm-feature-rowheader">基于高精度的3D触觉数字化<br>探索各类物品特征</div>
                    <div class="palm-feature-column">
                            <img src="${theme_base!}/assets/images/palm-desc.png" alt="">
                    </div>
                </div>
        </div>
        <div class="section palm-industry">
                <div class="industry-title">
                多种行业应用
                </div>
            <div class="industry-table">
                <div class="industry">
                    <div class="industry-header">
                        <img src="${theme_base!}/assets/images/plane.png" /> 航空
                    </div>
                    <div class="industry-desc">
                        发动机表面缺陷<br>3D数字化
                    </div>
                    <div class="industry-img-before">
                        <img src="${theme_base!}/assets/images/company-products/plane.png" alt="">
                    </div>
                    <div class="industry-img-after">
                        <img src="${theme_base!}/assets/images/company-products/plane.png" alt="">
                    </div>
                </div>
                <div class="industry">
                    <div class="industry-header">
                        <img src="${theme_base!}/assets/images/furniture.png" /> 家具
                    </div>
                    <div class="industry-desc">
                        家具表面缺陷<br>3D数字化
                    </div>
                    <div class="industry-img-before">
                        <img src="${theme_base!}/assets/images/company-products/furniture.png" alt="">
                    </div>
                    <div class="industry-img-after">
                        <img src="${theme_base!}/assets/images/company-products/furniture1.png" alt="">
                    </div>
                </div>
                <div class="industry">
                    <div class="industry-header">
                        <img src="${theme_base!}/assets/images/clothesl.png" /> 服装
                    </div>
                    <div class="industry-desc">
                        织物纹理表面<br>3D数字化
                    </div>
                    <div class="industry-img-before">
                        <img src="${theme_base!}/assets/images/company-products/clothes.png" alt="">
                    </div>
                    <div class="industry-img-after">
                        <img src="${theme_base!}/assets/images/company-products/clothes1.png" alt="">
                    </div>
                </div>
                <div class="industry">
                    <div class="industry-header">
                        <img src="${theme_base!}/assets/images/3c.png" /> 3C
                    </div>
                    <div class="industry-desc">
                        零配件、装配<br>质量检测
                    </div>
                    <div class="industry-img-before">
                        <img src="${theme_base!}/assets/images/company-products/phone.png" alt="">
                    </div>
                    <div class="industry-img-after">
                        <img src="${theme_base!}/assets/images/company-products/phone1.png" alt="">
                    </div>
                </div>
                <div class="industry">
                    <div class="industry-header">
                        <img src="${theme_base!}/assets/images/luxury.png" /> 鉴定
                    </div>
                    <div class="industry-desc">
                        高端奢侈品鉴定
                    </div>
                    <div class="industry-img-before">
                        <img src="${theme_base!}/assets/images/company-products/luxury.png" alt="">
                    </div>
                    <div class="industry-img-after">
                        <img src="${theme_base!}/assets/images/company-products/luxury1.png" alt="">
                    </div>
                </div>
            </div>    
        </div>
        <div class="section palm-materials">
                <div class="materials-title">
                针对不同的织物可清晰对表面进行数字化
                </div>
            <div class="materials-table">
                <div class="industry">
                    <div class="industry-img-before">
                        <img src="${theme_base!}/assets/images/clothes/clothes1.png" alt="">
                    </div>
                    <div class="industry-img-after">
                        <img src="${theme_base!}/assets/images/clothes/clothes1-after.png" alt="">
                    </div>
                </div>
                <div class="industry">
                    <div class="industry-img-before">
                        <img src="${theme_base!}/assets/images/clothes/clothes2.png" alt="">
                    </div>
                    <div class="industry-img-after">
                        <img src="${theme_base!}/assets/images/clothes/clothes2-after.png" alt="">
                    </div>
                </div>
                <div class="industry">
                    <div class="industry-img-before">
                        <img src="${theme_base!}/assets/images/clothes/clothes3.png" alt="">
                    </div>
                    <div class="industry-img-after">
                        <img src="${theme_base!}/assets/images/clothes/clothes3-after.png" alt="">
                    </div>
                </div>
                <div class="industry">
                    <div class="industry-img-before">
                        <img src="${theme_base!}/assets/images/clothes/clothes4.png" alt="">
                    </div>
                    <div class="industry-img-after">
                        <img src="${theme_base!}/assets/images/clothes/clothes4-after.png" alt="">
                    </div>
                </div>
            </div>
        </div>
        <div class="section palm-techspecs">
                <div class="techspecs-title">
                性能参数
                </div>
                <div class="techspecs-content">
                <div class="techspecs-row">
                    <div class="techspecs-rowheader">型号</div>
                    <div class="techspecs-column">PALM-1-20</div>
                </div>
            </div>
            <div class="techspecs-content">
                <div role="row" class="techspecs-row">
                    <div role="rowheader" class="techspecs-rowheader">XY工作范围</div>
                    <div role="cell" class="techspecs-column" aria-colspan="2">
                        Φ20mm
                    </div>
                </div>
            </div>
            <div class="techspecs-content">
                <div role="row" class="techspecs-row">
                    <div role="rowheader" class="techspecs-rowheader">XY轴精度</div>
                    <div role="cell" class="techspecs-column" aria-colspan="2">
                        20μm
                    </div>
                </div>
            </div>
            <div class="techspecs-content">
                <div role="row" class="techspecs-row">
                    <div role="rowheader" class="techspecs-rowheader">Z工作范围</div>
                    <div role="cell" class="techspecs-column" aria-colspan="2">
                        0-3mm
                    </div>
                </div>
            </div>
            <div class="techspecs-content">
                <div role="row" class="techspecs-row">
                    <div role="rowheader" class="techspecs-rowheader">Z轴精度</div>
                    <div role="cell" class="techspecs-column" aria-colspan="2">
                        50μm
                    </div>
                </div>
            </div>
            <div class="techspecs-content">
                <div role="row" class="techspecs-row">
                    <div role="rowheader" class="techspecs-rowheader">响应时间</div>
                    <div role="cell" class="techspecs-column" aria-colspan="2">
                        70ms
                    </div>
                </div>
            </div>
            <div class="techspecs-content">
                <div role="row" class="techspecs-row">
                    <div role="rowheader" class="techspecs-rowheader">运行温度</div>
                    <div role="cell" class="techspecs-column" aria-colspan="2">
                        0-40°C
                    </div>
                </div>
            </div>
            <div class="techspecs-content">
                <div role="row" class="techspecs-row">
                    <div role="rowheader" class="techspecs-rowheader">动态</div>
                    <div role="cell" class="techspecs-column" aria-colspan="2">
                        15FPS
                    </div>
                </div>
            </div>
            <div class="techspecs-content">
                <div role="row" class="techspecs-row">
                    <div role="rowheader" class="techspecs-rowheader">存储温度</div>
                    <div role="cell" class="techspecs-column" aria-colspan="2">
                        -20-60°C
                    </div>
                </div>
            </div>
            <div class="techspecs-content">
                <div role="row" class="techspecs-row">
                    <div role="rowheader" class="techspecs-rowheader">供电电压</div>
                    <div role="cell" class="techspecs-column" aria-colspan="2">
                        5V
                    </div>
                </div>
            </div>
            <div class="techspecs-content">
                <div role="row" class="techspecs-row">
                    <div role="rowheader" class="techspecs-rowheader">供电电流</div>
                    <div role="cell" class="techspecs-column" aria-colspan="2">
                        &lt;600mA
                    </div>
                </div>
            </div>
            <div class="techspecs-content">
                <div role="row" class="techspecs-row">
                    <div role="rowheader" class="techspecs-rowheader">运行环境</div>
                    <div role="cell" class="techspecs-column" aria-colspan="2">
                        Windows7以上版本
                    </div>
                </div>
            </div>
            <div class="techspecs-content">
                <div role="row" class="techspecs-row">
                    <div role="rowheader" class="techspecs-rowheader">接口方式</div>
                    <div role="cell" class="techspecs-column" aria-colspan="2">
                        USB3.0
                    </div>
                </div>
            </div>
        </div>
    </div>
<@foot customjs="company-product"/>